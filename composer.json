{
    "name": "meesterpaul/craft-stripe-webhooks",
    "description": "Handle Stripe webhooks in a CraftCMS application",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "stripe webhooks"
    ],
    "support": {
        "docs": "https://github.com/MeesterPaul/craft-stripe-webhooks/blob/master/README.md",
        "issues": "https://github.com/MeesterPaul/craft-stripe-webhooks/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Ratcliffe",
            "homepage": "https://www.refinedpractice.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "stripe/stripe-php": "^7.0"
    },
    "autoload": {
        "psr-4": {
          "MeesterPaul\\StripeWebhooks\\": "src/"
        }
    },
    "extra": {
        "name": "Stripe Webhooks",
        "handle": "stripe-webhooks-mp",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/MeesterPaul/craft-stripe-webhooks/master/CHANGELOG.md",
        "class": "MeesterPaul\\StripeWebhooks\\StripeWebhooks"
    }
}
